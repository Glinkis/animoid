{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": {
      "severity": "error",
      "options": "never-prefix"
    },
    "semicolon": {
      "severity": "error",
      "options": "never"
    },
    "object-literal-sort-keys": false
  }
}
