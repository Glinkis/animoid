{
  "name": "animoid",
  "version": "0.1.0",
  "description": "A web aimation utility.",
  "main": "index.js",
  "scripts": {
    "start": "rimraf dist && webpack-dev-server --config ./config/webpack.dev.js",
    "build": "rimraf dist && webpack --config ./config/webpack.prod.js",
    "deploy": "yarn lint && yarn build && gh-pages -d dist",
    "lint": "tslint config/**/*.ts src/**/*.js src/**/*.tsx",
    "cypress:open": "cypress open",
    "cypress": "cypress run",
    "test:open": "run-p --race start cypress:open",
    "test": "run-p --race start cypress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --write"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Glinkis/animoid.git"
  },
  "keywords": [
    "animation",
    "web"
  ],
  "author": "Victor Glind√•s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glinkis/animoid/issues"
  },
  "homepage": "https://glinkis.github.io/animoid/",
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@cypress/webpack-preprocessor": "^3.0.0",
    "@types/cypress": "^1.1.3",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "babel-loader": "^8.0.2",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "css-loader": "^1.0.0",
    "cypress": "^3.1.0",
    "gh-pages": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "prettier-tslint": "^0.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
